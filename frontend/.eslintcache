_ADDRESS } from \"../constants/CartConstant\";\n\nexport const cartReducer = (state = {cartItems: []} ,action) => {\n    switch(action.type){\n\n        case CART_ADD_ITEM:\n            const item = action.payload;\n            const existitem = state.cartItems.find((x)=> x.product === item.product);\n            if(existitem){\n                return {\n                    ...state,\n                    cartItems: state.cartItems.map((x) => x.product === existitem.product\n                        ? item\n                        : x)\n                }\n            }\n            else{\n                return {\n                    ...state,\n                    cartItems: [...state.cartItems, item]\n                }\n            }\n\n        case CART_REMOVE_ITEM:\n            return {...state, \n            cartItems: state.cartItems.filter((x)=>x.product != action.payload),\n            };\n\n        case CART_SAVE_SHIPPING_ADDRESS:\n            return {\n                ...state,\n                shippingAddress: action.payload\n            };\n\n        case CART_SAVE_PAYMENT_METHOD:\n            return {\n                ...state,\n                paymentMethod : action.payload\n            }\n        \n        case CART_EMPTY:\n            return {\n                ...state,\n                cartItems: []\n            }\n        \n        default:\n            return state;\n\n        \n    }\n}","C:\\Users\\sreeja\\Desktop\\Amazon-Clone-master\\Amazon-Clone-master\\amazon-frontend\\src\\reducers\\OrderReducer.js",[],"C:\\Users\\sreeja\\Desktop\\Amazon-Clone-master\\Amazon-Clone-master\\amazon-frontend\\src\\reducers\\UserReducer.js",[],"C:\\Users\\sreeja\\Desktop\\Amazon-Clone-master\\Amazon-Clone-master\\amazon-frontend\\src\\reducers\\ProductReducer.js",[],"C:\\Users\\sreeja\\Desktop\\Amazon-Clone-master\\Amazon-Clone-master\\amazon-frontend\\src\\pages\\Home.js",["173","174"],"import React,{useEffect} from 'react'\nimport { useSelector , useDispatch} fro